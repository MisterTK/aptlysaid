{
  "name": "review-management-template",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "build:clean": "rm -rf build .svelte-kit && npm run build",
    "ci": "npm run lint && npm run build",
    "prepare": "svelte-kit sync",
    "postinstall": "svelte-kit sync",
    "validate": "npm run format:check && npm run lint && npm run typecheck && npm run test:run",
    "precommit": "npm run validate"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-vercel": "^5.10.2",
    "@sveltejs/kit": "^2.39.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@types/glob": "^9.0.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.1.10",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.3",
    "fuse.js": "^7.1.0",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "html-to-text": "^9.0.5",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "super-sitemap": "^1.0.5",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "dependencies": {
    "@ai-sdk/google-vertex": "^3.0.26",
    "@ai-sdk/react": "^2.0.44",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/auth-ui-svelte": "^0.2.9",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "ai": "^5.0.44",
    "dotenv": "^17.2.2",
    "handlebars": "^4.7.8",
    "lucide-svelte": "^0.544.0",
    "node-fetch": "^3.3.2",
    "resend": "^6.0.3",
    "stripe": "^18.5.0"
  },
  "overrides": {
    "@supabase/auth-ui-svelte": {
      "svelte": "$svelte"
    },
    "@supabase/ssr": {
      "cookie": "^0.7.0"
    },
    "@sveltejs/kit": {
      "cookie": "^0.7.0"
    }
  }
}
